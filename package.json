  "bugs": {
    "url": "https://github.com/MrRayures/crossbones-customwork/issues"
  },
  "homepage": "https://github.com/MrRayures/crossbones-customwork#readme",
  "devDependencies": {
    "sass": "^1.26.5",
    "npm-run-all": "^4.1.5",
    "recursive-fs": "^2.1.0",
    "onchange": "^7.1.0",
    "@11ty/eleventy": "^2.0.0"
  }
}
